    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/immutable": "^3.8.7",
    "@types/lodash": "^4.14.120",
    "@types/mime-types": "^2.1.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.26",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/uuid": "^3.4.5",
    "@wordpress/wordcount": "^2.7.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "bowser": "^2.9.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "html-to-text": "^5.1.1",
    "immutable": "^4.0.0-rc.12",
    "keycloak-connect": "^8.0.0",
    "knex": "^0.19.3",
    "lodash": "^4.17.19",
    "mime-types": "^2.1.22",
    "moment": "^2.24.0",
    "multiparty": "^4.2.1",
    "nodemailer": "^6.3.1",
    "openid-client": "^3.7.1",
    "pg": "^7.12.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sha.js": "^2.4.11",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.7.4",
    "uuid": "^3.3.3"
